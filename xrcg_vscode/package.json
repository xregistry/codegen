  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "node esbuild.js",
    "esbuild": "node esbuild.js",
    "esbuild-extension": "node -e \"require('esbuild').buildSync({entryPoints:['src/extension.ts'],bundle:true,outfile:'dist/extension.js',external:['vscode'],format:'cjs',platform:'node',sourcemap:true})\"",
    "esbuild-watch": "npm run esbuild-base -- --watch",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run esbuild-extension && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "test-compile": "tsc -p ./"
  },
