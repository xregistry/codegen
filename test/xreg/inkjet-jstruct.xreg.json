                "properties": {
                  "tenantid": {
                    "type": "string",
                    "description": "Tenant identifier"
                  },
                  "deviceid": {
                    "type": "string",
                    "description": "Device identifier"
                  },
                  "cartridgeId": {
                    "type": "string",
                    "description": "Identifier of the low ink cartridge"
                  },
                  "inkColor": {
                    "type": "string",
                    "enum": ["Black", "Cyan", "Magenta", "Yellow"],
                    "description": "Color of the ink that is low"
                  },
                  "remainingLevel": {
                    "type": "int32",
                    "description": "Remaining ink level percentage"
                  }
                },
                "required": ["tenantid", "deviceid", "cartridgeId", "inkColor", "remainingLevel"],
                "definitions": {
                  "InkColor": {
                    "type": "string",
                    "enum": ["Black", "Cyan", "Magenta", "Yellow"],
                    "description": "Available ink colors"
                  }
                }
              }
            }
          }
        },
