                  },
                  "urgency": {
                    "type": "string",
                    "enum": ["Immediate", "Routine"],
                    "description": "Urgency of the maintenance required (e.g., 'immediate', 'routine')"
                  }
                },
                "required": ["tenantid", "deviceid", "maintenanceType", "urgency"],
                "definitions": {
                  "MaintenanceType": {
                    "type": "string",
                    "enum": ["Cleaning", "MechanicalAdjustment"],
