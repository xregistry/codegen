  "schemagroups": {
    "Fabrikam.InkJetPrinter": {
      "schemas": {
        "Fabrikam.InkJetPrinter.PrintJobStartedEventData": {
          "format": "JSONStructure/draft-02",
          "defaultversionid": "1",
          "versions": {
            "1": {
              "format": "JSONStructure/draft-02",
              "schema": {
                "$schema": "https://json-structure.org/meta/core/v0/#",
                "$id": "https://fabrikam.com/inkjet/PrintJobStartedEventData",
                "name": "PrintJobStartedEventData",
                "description": "Data schema for starting a print job",
                "type": "object",
                "properties": {
                  "tenantid": {
                    "type": "string",
                    "description": "Tenant identifier"
                  },
                  "deviceid": {
                    "type": "string",
                    "description": "Device identifier"
                  },
                  "jobId": {
                    "type": "string",
                    "description": "Unique identifier for the print job"
                  },
                  "documentName": {
                    "type": "string",
                    "description": "Name of the document being printed"
                  },
                  "startTime": {
                    "type": "datetime",
                    "description": "Timestamp when the print job started"
                  }
                },
                "required": ["tenantid", "deviceid", "jobId", "documentName", "startTime"]
              }
            }
          }
        },
        "Fabrikam.InkJetPrinter.PrintJobCompletedEventData": {
          "format": "JSONStructure/draft-02",
          "defaultversionid": "1",
          "versions": {
            "1": {
              "format": "JSONStructure/draft-02",
              "schema": {
                "$schema": "https://json-structure.org/meta/core/v0/#",
                "$id": "https://fabrikam.com/inkjet/PrintJobCompletedEventData",
                "name": "PrintJobCompletedEventData",
                "description": "Data schema for a completed print job",
                "type": "object",
                "properties": {
                  "tenantid": {
                    "type": "string",
                    "description": "Tenant identifier"
                  },
                  "deviceid": {
                    "type": "string",
                    "description": "Device identifier"
                  },
                  "jobId": {
                    "type": "string",
                    "description": "Unique identifier for the completed print job"
                  },
                  "documentName": {
                    "type": "string",
                    "description": "Name of the document printed"
                  },
                  "completionTime": {
                    "type": "datetime",
                    "description": "Timestamp when the print job was completed"
                  },
                  "outcome": {
                    "type": "string",
                    "enum": ["Successful", "Failed", "Cancelled"],
                    "description": "Outcome of the print job (successful, failed, cancelled)"
                  }
                },
                "required": ["tenantid", "deviceid", "jobId", "documentName", "completionTime", "outcome"],
                "definitions": {
                  "PrintJobOutcome": {
                    "type": "string",
                    "enum": ["Successful", "Failed", "Cancelled"],
                    "description": "Possible outcomes for a print job"
                  }
                }
              }
            }
          }
        },
